  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext"
  },
  "include": ["../config/prettier.config.cjs"]
}
